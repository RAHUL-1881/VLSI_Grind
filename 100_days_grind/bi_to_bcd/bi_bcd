    %store/vec4 v0x556a59853c30_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x556a59853f00_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x556a59853f00_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x556a59853c30_0;
    %load/vec4 v0x556a59853b50_0;
    %load/vec4 v0x556a59853f00_0;
    %part/s 1;
    %pad/u 32;
    %pushi/vec4 2, 0, 32;
    %load/vec4 v0x556a59853f00_0;
    %pow/s;
    %mul;
    %add;
    %store/vec4 v0x556a59853c30_0, 0, 32;
    %load/vec4 v0x556a59853f00_0;
    %addi 1, 0, 32;
    %store/vec4 v0x556a59853f00_0, 0, 32;

    %load/vec4 v0x556a59853c30_0;
    %pushi/vec4 10, 0, 32;
    %mod/s;
    %pad/s 4;
    %ix/getv/s 4, v0x556a59853f00_0;
    %store/vec4a v0x556a59853cf0, 4, 0;
    %load/vec4 v0x556a59853c30_0;
    %pushi/vec4 10, 0, 32;
    %div/s;
    %store/vec4 v0x556a59853c30_0, 0, 32;
    %load/vec4 v0x556a59853f00_0;
    %addi 1, 0, 32;
    %store/vec4 v0x556a59853f00_0, 0, 32;
    %jmp T_1.2;

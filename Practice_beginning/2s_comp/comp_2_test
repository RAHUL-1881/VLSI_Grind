 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "out";
    .port_info 1 /INPUT 4 "a";
v0x561bd6dbabf0_0 .net "a", 3 0, v0x561bd6e02d10_0;  1 drivers
v0x561bd6dbb000_0 .var "out", 3 0;
E_0x561bd6db9bb0 .event edge, v0x561bd6dbabf0_0;
    .scope S_0x561bd6df29a0;
T_0 ;
    %wait E_0x561bd6db9bb0;
    %load/vec4 v0x561bd6dbabf0_0;
    %inv;
    %addi 1, 0, 4;
    %assign/vec4 v0x561bd6dbb000_0, 0;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x561bd6df2810;
T_1 ;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x561bd6e02d10_0, 0, 4;
    %delay 20, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x561bd6e02d10_0, 0, 4;
    %delay 20, 0;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x561bd6e02d10_0, 0, 4;
    %delay 40, 0;
    %vpi_call 2 11 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x561bd6df2810;
T_2 ;
    %vpi_call 2 15 "$dumpfile", "comp_2_test.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars" {0 0 0};
    %end;
    .thread T_2;

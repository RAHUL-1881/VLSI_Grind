#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x56225887aba0 .scope module, "mux21_if_tb" "mux21_if_tb" 2 1;
 .timescale 0 0;
v0x56225888d550_0 .var "i", 1 0;
v0x56225888d640_0 .net "out", 0 0, v0x562258843000_0;  1 drivers
v0x56225888d710_0 .var "sel", 0 0;
S_0x56225887ad30 .scope module, "gh" "mux21_if" 2 6, 3 1 0, S_0x56225887aba0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "out";
    .port_info 1 /INPUT 2 "i";
    .port_info 2 /INPUT 1 "sel";
v0x562258842bf0_0 .net "i", 1 0, v0x56225888d550_0;  1 drivers
v0x562258843000_0 .var "out", 0 0;
v0x56225888d430_0 .net "sel", 0 0, v0x56225888d710_0;  1 drivers
E_0x5622588417b0 .event edge, v0x56225888d430_0, v0x562258842bf0_0;
    .scope S_0x56225887ad30;
T_0 ;
    %wait E_0x5622588417b0;
    %load/vec4 v0x56225888d430_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x562258842bf0_0;
    %parti/s 1, 1, 2;
    %assign/vec4 v0x562258843000_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x562258842bf0_0;
    %parti/s 1, 0, 2;
    %assign/vec4 v0x562258843000_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x56225887aba0;
T_1 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x56225888d550_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56225888d710_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x56225888d550_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56225888d710_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x56225888d550_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56225888d710_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x56225888d550_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56225888d710_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x56225888d550_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56225888d710_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x56225888d550_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56225888d710_0, 0, 1;
    %delay 1, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x56225887aba0;
T_2 ;
    %vpi_call 2 18 "$dumpfile", "mux21_test.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "mux21_if_tb.sv";

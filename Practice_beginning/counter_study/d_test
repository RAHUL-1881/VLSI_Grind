#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x56174b976c00 .scope module, "d_ff_tb" "d_ff_tb" 2 1;
 .timescale 0 0;
v0x56174b988ca0_0 .var "clk", 0 0;
v0x56174b988d60_0 .var "d", 0 0;
v0x56174b988e30_0 .net "q", 0 0, v0x56174b988a60_0;  1 drivers
v0x56174b988f30_0 .net "qbar", 0 0, v0x56174b988b30_0;  1 drivers
S_0x56174b976d90 .scope module, "gh" "d_ff" 2 4, 3 1 0, S_0x56174b976c00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "d";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 1 "q";
    .port_info 3 /OUTPUT 1 "qbar";
v0x56174b93ebf0_0 .net "clk", 0 0, v0x56174b988ca0_0;  1 drivers
v0x56174b93f000_0 .net "d", 0 0, v0x56174b988d60_0;  1 drivers
v0x56174b988a60_0 .var "q", 0 0;
v0x56174b988b30_0 .var "qbar", 0 0;
E_0x56174b93d920 .event posedge, v0x56174b93ebf0_0;
    .scope S_0x56174b976d90;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56174b988a60_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56174b988b30_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x56174b976d90;
T_1 ;
    %wait E_0x56174b93d920;
    %load/vec4 v0x56174b93f000_0;
    %store/vec4 v0x56174b988a60_0, 0, 1;
    %load/vec4 v0x56174b988a60_0;
    %inv;
    %store/vec4 v0x56174b988b30_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x56174b976c00;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56174b988ca0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56174b988d60_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56174b988d60_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56174b988d60_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56174b988d60_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56174b988d60_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56174b988d60_0, 0, 1;
    %delay 25, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x56174b976c00;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x56174b988ca0_0;
    %inv;
    %store/vec4 v0x56174b988ca0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x56174b976c00;
T_4 ;
    %vpi_call 2 19 "$dumpfile", "d_ff.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "d_ff_tb.sv";
    "d_ff.sv";

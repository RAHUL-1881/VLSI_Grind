#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55e882e98ad0 .scope module, "my_counter_tb" "my_counter_tb" 2 1;
 .timescale 0 0;
v0x55e882eaaca0_0 .var "clk", 0 0;
v0x55e882eaad70_0 .net "count", 3 0, v0x55e882e61000_0;  1 drivers
v0x55e882eaae40_0 .var "rst", 0 0;
S_0x55e882e98c60 .scope module, "ha" "my_counter" 2 5, 3 1 0, S_0x55e882e98ad0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "out";
v0x55e882e60bf0_0 .net "clk", 0 0, v0x55e882eaaca0_0;  1 drivers
v0x55e882e61000_0 .var "out", 3 0;
v0x55e882eaab80_0 .net "rst", 0 0, v0x55e882eaae40_0;  1 drivers
E_0x55e882e5fc70 .event posedge, v0x55e882e60bf0_0;
    .scope S_0x55e882e98c60;
T_0 ;
    %wait E_0x55e882e5fc70;
    %load/vec4 v0x55e882eaab80_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55e882e61000_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55e882e61000_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55e882e61000_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55e882e98ad0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e882eaaca0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e882eaae40_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e882eaae40_0, 0, 1;
    %delay 2000, 0;
    %vpi_call 2 10 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55e882e98ad0;
T_2 ;
    %delay 50, 0;
    %load/vec4 v0x55e882eaaca0_0;
    %inv;
    %store/vec4 v0x55e882eaaca0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55e882e98ad0;
T_3 ;
    %vpi_call 2 14 "$dumpfile", "count.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars" {0 0 0};
    %vpi_call 2 16 "$monitor", "%d,%b,%b,%d", $time, v0x55e882eaaca0_0, v0x55e882eaae40_0, v0x55e882eaad70_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "my_counter_tb.v";
    "my_counter.v";

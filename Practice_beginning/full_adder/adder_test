:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x561ce106a5d0 .scope module, "full_adder_tb" "full_adder_tb" 2 1;
 .timescale 0 0;
v0x561ce1090c60_0 .var "a", 3 0;
v0x561ce1090d40_0 .var "b", 3 0;
v0x561ce1090de0_0 .var "cin", 0 0;
v0x561ce1090ee0_0 .net "cout", 0 0, v0x561ce10908f0_0;  1 drivers
v0x561ce1090fb0_0 .net "sum", 3 0, v0x561ce1090ae0_0;  1 drivers
S_0x561ce107e1f0 .scope module, "DUT" "full_adder" 2 4, 3 1 0, S_0x561ce106a5d0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /INPUT 4 "b";
    .port_info 2 /INPUT 1 "cin";
    .port_info 3 /OUTPUT 4 "sum";
    .port_info 4 /OUTPUT 1 "cout";
v0x561ce1044bf0_0 .net "a", 3 0, v0x561ce1090c60_0;  1 drivers
v0x561ce1045000_0 .net "b", 3 0, v0x561ce1090d40_0;  1 drivers
v0x561ce1090790_0 .var "carry", 0 0;
v0x561ce1090830_0 .net "cin", 0 0, v0x561ce1090de0_0;  1 drivers
v0x561ce10908f0_0 .var "cout", 0 0;
v0x561ce1090a00_0 .var "i", 2 0;
v0x561ce1090ae0_0 .var "sum", 3 0;
E_0x561ce107c1f0 .event edge, v0x561ce1090830_0, v0x561ce1044bf0_0, v0x561ce1045000_0, v0x561ce1090790_0;
    .scope S_0x561ce107e1f0;
T_0 ;
    %wait E_0x561ce107c1f0;
    %load/vec4 v0x561ce1090830_0;
    %store/vec4 v0x561ce1090790_0, 0, 1;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x561ce1090a00_0, 0, 3;
T_0.0 ;
    %load/vec4 v0x561ce1090a00_0;
    %pad/u 32;
    %cmpi/u 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x561ce1044bf0_0;
    %load/vec4 v0x561ce1090a00_0;
    %part/u 1;
    %pad/u 2;
    %load/vec4 v0x561ce1045000_0;
    %load/vec4 v0x561ce1090a00_0;
    %part/u 1;
    %pad/u 2;
    %add;
    %load/vec4 v0x561ce1090790_0;
    %pad/u 2;
    %add;
    %split/vec4 1;
    %ix/getv 4, v0x561ce1090a00_0;
    %store/vec4 v0x561ce1090ae0_0, 4, 1;
    %store/vec4 v0x561ce1090790_0, 0, 1;
    %load/vec4 v0x561ce1090a00_0;
    %addi 1, 0, 3;
    %store/vec4 v0x561ce1090a00_0, 0, 3;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x561ce1090790_0;
    %store/vec4 v0x561ce10908f0_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x561ce106a5d0;
T_1 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x561ce1090c60_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x561ce1090d40_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561ce1090de0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x561ce106a5d0;
T_2 ;
    %vpi_call 2 25 "$dumpfile", "adder_tb.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x561ce106a5d0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;

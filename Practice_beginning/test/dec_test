S_0x5604bbc55060 .scope module, "dec_tb" "dec_tb" 2 1;
 .timescale 0 0;
v0x5604bbc66530_0 .var "binary_in", 31 0;
v0x5604bbc66620_0 .net "decimal_out", 31 0, L_0x5604bbc666f0;  1 drivers
S_0x5604bbc551f0 .scope module, "gh" "dec" 2 5, 3 1 0, S_0x5604bbc55060;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "binary_in";
    .port_info 1 /OUTPUT 32 "decimal_out";
L_0x5604bbc666f0 .functor BUFZ 32, v0x5604bbc66250_0, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x5604bbc1c000_0 .net "binary_in", 31 0, v0x5604bbc66530_0;  1 drivers
v0x5604bbc66250_0 .var/i "decimal", 31 0;
v0x5604bbc66330_0 .net "decimal_out", 31 0, L_0x5604bbc666f0;  alias, 1 drivers
v0x5604bbc663f0_0 .var/i "i", 31 0;
E_0x5604bbc1a780 .event edge, v0x5604bbc1c000_0;
    .scope S_0x5604bbc551f0;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5604bbc663f0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5604bbc66250_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x5604bbc551f0;
T_1 ;
    %wait E_0x5604bbc1a780;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5604bbc66250_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5604bbc663f0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x5604bbc663f0_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x5604bbc66250_0;
    %load/vec4 v0x5604bbc1c000_0;
    %load/vec4 v0x5604bbc663f0_0;
    %part/s 1;
    %pad/u 32;
    %pushi/vec4 2, 0, 32;
    %load/vec4 v0x5604bbc663f0_0;
    %pow/s;
    %mul;
    %add;
    %store/vec4 v0x5604bbc66250_0, 0, 32;
    %load/vec4 v0x5604bbc663f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5604bbc663f0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5604bbc55060;
T_2 ;
    %pushi/vec4 4294967295, 0, 32;
    %store/vec4 v0x5604bbc66530_0, 0, 32;
    %delay 5, 0;
    %vpi_call 2 9 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5604bbc55060;
T_3 ;
    %vpi_call 2 13 "$dumpfile", "dec.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars" {0 0 0};
    %end;
    .thread T_3;
